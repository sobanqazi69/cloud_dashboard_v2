# Firebase Admin SDK Configuration
# Get these values from your Firebase Console > Project Settings > Service Accounts
# Generate a private key and copy the values here

FIREBASE_TYPE=service_account
FIREBASE_PROJECT_ID=node-red-75bfc
FIREBASE_PRIVATE_KEY_ID=your-private-key-id-here
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@node-red-75bfc.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your-client-id-here
FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token
FIREBASE_AUTH_PROVIDER_X509_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
FIREBASE_CLIENT_X509_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/firebase-adminsdk-xxxxx%40node-red-75bfc.iam.gserviceaccount.com

# Firebase Realtime Database URL
FIREBASE_DATABASE_URL=https://node-red-75bfc-default-rtdb.firebaseio.com 